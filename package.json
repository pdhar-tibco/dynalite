{
  "name": "dynalite-filedown",
  "version": "1.2.0",
  "description": "An implementation of Amazon's DynamoDB built on serialized RBT",
  "main": "index.js",
  "bin": "cli.js",
  "scripts": {
    "build": "for file in ./db/*.pegjs; do pegjs \"$file\"; done",
    "test": "mocha --require should --reporter spec -t $([ $REMOTE ] && echo 30s || echo 4s)",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --require should -t 4s"
  },
  "repository": "mhart/dynalite",
  "keywords": [
    "dynamodb",
    "mock",
    "fake",
    "test",
    "aws",
    "dynamo",
    "leveldb"
  ],
  "author": "Pranab Dhar<pranab.dhar@gmail.com>, Michael Hart <michael.hart.au@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "abstract-leveldown": "^4.0.2",
    "async": "^2.1.4",
    "big.js": "^3.1.3",
    "buffer-crc32": "^0.2.13",
    "circular-json": "^0.5.1",
    "del": "^3.0.0",
    "encoding-down": "^4.0.0",
    "graceful-fs": "^4.1.11",
    "immediate": "^3.2.3",
    "inherits": "^2.0.3",
    "iota-array": "^1.0.0",
    "lazy": "^1.0.11",
    "level-sublevel": "^6.6.1",
    "leveldown": "^3.0.0",
    "levelup": "^2.0.2",
    "lock": "^1.1.0",
    "ltgt": "^2.2.0",
    "memdown": "^1.2.4",
    "minimist": "^1.2.0",
    "once": "^1.4.0",
    "safe-buffer": "^5.1.1",
    "tape": "^4.8.0",
    "util": "0.10.3",
    "write": "^1.0.3"
  },
  "optionalDependencies": {
    "leveldown": "^3.0.0"
  },
  "devDependencies": {
    "aws4": "^1.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "pegjs": "^0.10.0",
    "should": "^11.1.2"
  }
}
